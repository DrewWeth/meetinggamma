<h1>Listing meetings</h1>

<table>
  <thead>
    <tr>
      <th>Identifier</th>
      <th>Filename</th>
      <th>Logfilename</th>
      <th>Time</th>
      <th>Cost</th>
      <th>Name</th>
      <th>Email</th>
      <th>worth</th>
      <th>People</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.identifier %></td>
        <td><%= meeting.filename %></td>
        <td><%= meeting.logFilename %></td>
        <td><%= meeting.time %></td>
        <td><%= meeting.cost %></td>
        <td><%= meeting.name %></td>
        <td><%= meeting.email %></td>
        <td><%= meeting.worth %></td>
        <td><%= meeting.people %></td>
        <td><%= meeting.user_id %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>
